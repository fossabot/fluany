{
  "presets": ["react", "es2015"],
  "plugins": [
    "transform-object-rest-spread",
    "transform-async-to-generator",
    "add-module-exports",
    ["module-resolver", {
       "alias": {
         "components": "./src/popup/components",
         "constants": "./src/popup/constants",
         "actions": "./src/popup/actions",
         "reducers": "./src/popup/reducers",
         "store": "./src/popup/store",
         "containers": "./src/popup/containers",
         "shared": "./src/shared/",
         "analytics": "./src/analytics/"
       }
    }],
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ]
}
